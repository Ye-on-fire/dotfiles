{
  "layer": "top", // Waybar at top layer
  "position": "top", // Waybar position (top|bottom|left|right)
  "height": 30,
  "margin": "0 0 0 0",
  //"width": 1350, // Waybar width
  // Choose the order of the modules idle_inhibitor
  "modules-left": [
    "custom/launcher",
    "niri/workspaces",
    "tray",
    "custom/cava-internal",
  ],
  "modules-center": ["niri/window","clock"],
  "modules-right": [
    "idle_inhibitor",
    "backlight",
    "pulseaudio",
    "temperature",
    "memory",
    "battery",
    "network",
    "custom/power"
  ],

  // Modules configuration

  "niri/workspaces": {
    "disable-scroll": true,
    "all-outputs": true,
    "format": "{icon}",
    // "format": "< %g >",
    // "format_focused" : "< %g ‚óè >",
    "format-icons": {
      "1": "‰∏Ä",
      "2": "‰∫å",
      "3": "‰∏â",
      "4": "Âõõ",
      "5": "‰∫î",
      "6": "ÂÖ≠",
      "7": "‰∏É",
      "8": "ÂÖ´",
      "9": "‰πù",
      "10": "ÂçÅ"
    }
  },
  "mpd": {
    "tooltip": true,
    "tooltip-format": "{artist} - {album} - {title} - Total Time : {totalTime:%M:%S}",
    "format": "Ôã≤ {elapsedTime:%M:%S}",
    "format-disconnected": "‚ö†  Disconnected",
    "format-stopped": "Ôã≤ Not Playing",
    "on-click": "mpc toggle",
    "state-icons": {
      "playing": "ÔÅã",
      "paused": "Ô££"
    }
  },
  "mpd#2": {
    "format": "ÔÅà",
    "format-disconnected": "ÔÅà",
    "format-paused": "ÔÅà",
    "format-stopped": "ÔÅà",
    // Commands to execute on events
    "on-click": "mpc -q pause && mpc -q prev && mpc -q start"
  },
  "mpd#3": {
    "interval": 1,
    "format": "{stateIcon}",
    "format-disconnected": "ÔÅã",
    "format-paused": "{stateIcon}",
    "format-stopped": "ÔÅã",
    "state-icons": {
      "paused": "ÔÅã",
      "playing": "ÔÅå"
    },
    // Commands to execute on events
    "on-click": "mpc toggle"
  },
  "mpd#4": {
    "format": "ÔÅë",
    "format-disconnected": "ÔÅë",
    "format-paused": "ÔÅë",
    "format-stopped": "ÔÅë",
    // Commands to execute on events
    "on-click": "mpc -q pause && mpc -q next && mpc -q start"
  },
  "custom/swap": {
    "on-click": "~/.config/waybar/scripts/swap.sh",
    "tooltip": "Swap between waybar configs",
    "format": "Bg ÔàÑ "
  },
  "custom/mpd": {
    "format": "ÔÄÅ {title} - {artist}",
    "interval": 5,
    "exec": "mpc current --format='\"title\":\"%title%\",\"artist\":\"%artist%\"'"
  },
  "custom/cava-internal": {
    "exec": "sh ~/.config/waybar/scripts/Cava.sh",
    "format": "{}",
    "tooltip": false,
    "layer": "below",
    "output": "all"
  },
  "niri/mode": {
    "format": "<span style=\"italic\">{}</span>"
  },
  "niri/window": {
    "format": "~ {app_id}"
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÅÆ ",
      "deactivated": "ÔÅ∞ "
    }
  },
  "tray": {
    "icon-size": 14,
    "spacing": 5
  },
  "clock": {
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>"
      }
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down"
    },
    "format": "ÔÑ≥  {:%a %d %b ÔÄó  %I:%M %p}", //12 hour format
    // "format": "ÔÑ≥ {:%d %m %Y ÔÄó %H:%M}",	//24 hour format
    "format-alt": "ÔÑ≥  {:%d/%m/%Y ÔÄó  %H:%M:%S}",
    // //"timezones": [ "Kolkata" ],
    // //"max-length": 200
    "interval": 1
  },
  // "clock": {
  //     "format": "ÔÑ≥  {:%H:%M:%S}",
  //     "format-alt": "ÔÑ≥  {:%A, %B %d, %Y (%R)}",
  //     "tooltip-format": "<tt><small>{calendar}</small></tt>",
  //     "calendar": {
  //         "mode"          : "month",
  //         "mode-mon-col"  : 3,
  //         "weeks-pos"     : "right",
  //         "on-scroll"     : 1,
  //         "on-click-right": "mode",
  //         "format": {
  //             "months":     "<span color='#ffead3'><b>{}</b></span>",
  //             "days":       "<span color='#ecc6d9'><b>{}</b></span>",
  //             "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
  //             "weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
  //             "today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
  //         }
  //     },
  //     "actions": {
  //         "on-click-right": "mode",
  //         "on-click-forward": "tz_up",
  //         "on-click-backward": "tz_down",
  //         "on-scroll-up": "shift_up",
  //         "on-scroll-down": "shift_down"
  //     },
  //     "interval": 1
  // },
  "cpu": {
    "format": "Ô¨ô{usage: >3}%",
    "on-click": "kitty -e btop"
  },
  "memory": {
    "format": "Ôî∏ {: >3}%",
    "on-click": "kitty -e btop"
  },
  "temperature": {
    // "thermal-zone": 2,
    "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
    "critical-threshold": 80,
    // "format-critical": "{temperatureC}¬∞C Ôãá",
    "format": "Ôãâ  {temperatureC}¬∞C"
  },
  "backlight": {
    // "device": "acpi_video1",
    "format": "{icon} {percent: >3}%",
    "format-icons": ["ÔÜÖ", "ÔÑë"],
    // "on-scroll-down": "brightnessctl -c backlight set 5%-",
    "on-scroll-down": "light -A 5 && light -G | cut -d'.' -f1 > $niriSOCK.wob",
    // "on-scroll-up": "brightnessctl -c backlight set +5%"
    "on-scroll-up": "light -U 5 && light -G | cut -d'.' -f1 > $niriSOCK.wob"
  },
  "battery": {
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "{icon}{capacity: >3}%",
    "format-charging": "Ôá¶ {icon}{capacity: >3}%",
    "format-plugged": "Ôá¶ {icon}{capacity: >3}%",
    //"format-good": "üòä {icon}{capacity: >3}%", // An empty format will hide the module
    //"format-full": "üòÑ {icon}{capacity: >3}%",
    "format-icons": ["ÔâÑ ", "ÔâÉ ", "ÔâÇ ", "ÔâÅ ", "ÔâÄ "],
    //"format-icons": ["Ôñç", "Ôïπ", "Ôïª", "Ôïº", "ÔïΩ", "Ôïæ", "Ôïø", "ÔñÄ", "ÔñÅ", "Ôï∏"]
    "interval": 5
  },
  "network": {
    // "interface": "wlp2s0", // (Optional) To force the use of this interface
    "format": "‚ö†  Disabled",
    "format-wifi": "Ôá´  {essid}",
    // "format-ethernet": "ÔÉÅ {ifname}: {ipaddr}/{cidr}",
    "format-ethernet": "ÔÉÅ  Wired",
    "format-disconnected": "‚ö†  Disconnected",
    // "on-click": "alacritty -e nmtui"
    "on-click": "better-control -w"
  },
  "pulseaudio": {
    "scroll-step": 1,
    "format": "{icon} {volume: >3}%",
    "format-bluetooth": "{icon} {volume: >3}%",
    "format-muted": "ÔÄ¶ muted",
    "format-icons": {
      "headphones": "ÔÄ•",
      "handsfree": "ÔÄ•",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄß", "ÔÄ®"]
    },
    "on-click": "better-control -V",
    "on-scroll-up": "pamixer -ui 2 && pamixer --get-volume > $niriSOCK.wob",
    "on-scroll-down": "pamixer -ud 2 && pamixer --get-volume > $niriSOCK.wob"
  },
  "custom/pacman": {
    "format": "ÔÜá {}",
    "interval": 7200, // every two hours
    "exec": "i=$(checkupdates); echo \"$i\" |wc -l; echo \"$i\" |column -t |tr '\n' '\r'", // # of updates and tooltip details
    "exec-if": "exit 0", // always run; consider advanced run conditions
    "on-click": "alacritty -e sudo pacman -Syu", // update system
    "signal": 8
  },
  "custom/power": {
    "format": "‚èª",
    "on-click": "exec wlogout",
    "tooltip": false
  },
  "custom/keyboard-layout": {
    "exec": "niri msg keyboard-layouts",
    // Interval set only as a fallback, as the value is updated by signal
    "interval": 1,
    "format": "ÔÑú  {}", // Icon: keyboard
    // Signal sent by niri key binding (~/.config/sway/key-bindings)
    "signal": 1, // SIGHUP
    "tooltip": false,
    "on-click": "~/.config/waybar/scripts/keyhint.sh"
  },
  "custom/launcher": {
    "format": "ÔÄï ",
    "on-click": "exec ~/.config/rofi/scripts/launcher_t2",
    "tooltip": false
  }
}
